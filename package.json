{
  "name": "@blasterjs/dev",
  "version": "0.0.1",
  "description": "React UI library designed and built by Azavea",
  "main": "build/index.js",
  "scripts": {
    "start": "yarn build && yarn bootstrap && NODE_ENV=development styleguidist server",
    "test": "jest --coverage --verbose --passWithNoTests",
    "bootstrap": "lerna bootstrap",
    "build": "rm -rf dist && lerna exec --parallel -- babel . -d dist --ignore node_modules,dist",
    "publish": "yarn run prepare-release && lerna publish",
    "prepare-release": "git checkout master && git pull --rebase origin master && yarn run build && lerna updated",
    "precommit": "yarn test",
    "postinstall": "yarn bootstrap",
    "contributor": "all-contributors add",
    "generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raster-foundry/blasterjs.git"
  },
  "author": "Gary Meehan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raster-foundry/blasterjs/issues"
  },
  "homepage": "https://github.com/raster-foundry/blasterjs",
  "devDependencies": {
    "all-contributors-cli": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.77.0",
    "glob-loader": "^0.3.0",
    "jest": "^23.4.1",
    "lerna": "^2.11.0",
    "lerna-changelog": "^0.8.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-styleguidist": "^7.0.19",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^3.2.1",
    "webpack": "^4.16.0",
    "webpack-blocks": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "resetModules": true,
    "testMatch": [
      "**/?(*.)(spec).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>node_modules/"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  }
}
